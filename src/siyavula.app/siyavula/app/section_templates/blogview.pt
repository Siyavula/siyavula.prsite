
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="siyavula.app">
    <body>
        <metal:siyavula_banner fill-slot="siyavula_banner" tal:define="normal_view nocall:context/@@view">
            <metal:siyavula_banner_macro use-macro="normal_view/macros/siyavula_banner" />
        </metal:siyavula_banner>
        <metal:siyavula_heading fill-slot="siyavula_heading" tal:define="normal_view nocall:context/@@view">
            <metal:siyavula_heading_macro use-macro="normal_view/macros/siyavula_heading" />
        </metal:siyavula_heading>
        <metal:siyavula_content fill-slot="siyavula_content" tal:define="normal_view nocall:context/@@view">
            <metal:siyavula_content_macro use-macro="normal_view/macros/siyavula_content" />
        </metal:siyavula_content>
    </body>
</html>

